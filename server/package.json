{
  "name": "server",
  "version": "0.0.0",
  "scripts": {
    "build": "node build.js && tsc --build tsconfig.prod.json",
    "lint": "tslint --project \"tsconfig.json\"",
    "start": "node -r module-alias/register ./dist --env=production",
    "start:dev": "ts-node -T -r tsconfig-paths/register ./src",
    "nodemon": "nodemon ",
    "tsc": "tsc",
    "test": "nodemon --config ./spec/nodemon.json",
    "test:ci": "ts-node -r tsconfig-paths/register ./spec",
    "ts-coverage": "typescript-coverage-report",
    "istanbul": "istanbul cover jasmine-node --captureExceptions spec/**/* ./node_modules/.bin/codecov",
    "coverage": "nyc npm run test:ci",
    "codecov": "nyc report --reporter=text-lcov > coverage.lcov"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "ts-node -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@daos": "dist/daos",
    "@entities": "dist/entities",
    "@shared": "dist/shared",
    "@server": "dist/Server",
    "@types": "dist/@types"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^8.5.0",
    "axios": "^0.20.0",
    "cockatiel": "^1.0.0",
    "command-line-args": "^5.1.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "etcd3": "^1.0.1",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^4.1.1",
    "http-status-codes": "^2.1.2",
    "jsonfile": "^6.0.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "tslib": "^2.0.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/find": "^0.2.1",
    "@types/helmet": "0.0.48",
    "@types/jasmine": "^3.5.14",
    "@types/jsonfile": "^6.0.0",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.10.1",
    "@types/supertest": "^2.0.10",
    "codecov": "^3.7.2",
    "find": "^0.3.0",
    "fs-extra": "^9.0.1",
    "istanbul": "^0.4.5",
    "jasmine": "^3.6.1",
    "jasmine-node": "^3.0.0",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "supertest": "^4.0.2",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.2",
    "typescript-coverage-report": "^0.1.3"
  }
}
